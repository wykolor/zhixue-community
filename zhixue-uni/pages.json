    },
    {
      // 测试界面
      "path": "pages/test/index",
      "style": {}
    },
    {
      // 外部链接
      "path": "pages/outUrl/outUrl",
      "style": {}
    },
    {
      // 授权用户信息
      "path": "pages/authUserInfo/authUserInfo",
      "style": {}
    },
    {
      // 授权手机号
      "path": "pages/authPhone/authPhone",
      "style": {}
    },
    {
      "path": "pages/articleDetail/articleDetail",
      "style": {}
    },
	{
	  "path": "pages/facemag/facemag",
	  "style": {
	    "navigationBarTitleText": "人脸管理",
	    "usingComponents": {
	      "van-uploader": "/wxcomponents/vant/uploader/index"
	    }
	  }
	},
    {
      "path": "pages/newsDetail/newsDetail",
      "style": {
        "usingComponents": {
          "van-cell": "/wxcomponents/vant/cell/index"
        }
      }
    },
    // 访客功能
    {
      "path": "pages/visitor/visitor",
